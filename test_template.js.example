const TEST_CONFIG = {
  // Replace with your actual API keys for testing
  trieveApiKey: 'your_trieve_api_key_here',
  groqApiKey: 'your_groq_api_key_here',
  datasetId: 'your_dataset_id_here',
  organizationId: 'your_organization_id_here',
  baseUrl: 'https://api.trieve.ai'
};

// Example usage:
async function testConnection() {
  try {
    const response = await fetch(`${TEST_CONFIG.baseUrl}/api/endpoint`, {
      headers: {
        'Authorization': `Bearer ${TEST_CONFIG.trieveApiKey}`,
        'Content-Type': 'application/json'
      }
    });
    
    if (response.ok) {
      console.log('✅ Connection successful');
    } else {
      console.log('❌ Connection failed');
    }
  } catch (error) {
    console.error('Error:', error);
  }
}

// Run test
testConnection();
